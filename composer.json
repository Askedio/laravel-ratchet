{
    "name": "askedio/laravel-ratchet",
    "description": "A Ratchet Server built for Laravel",
    "keywords": ["laravel"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1.0",
        "ext-zmq": "*",
        "cboden/ratchet": "^0.4",
        "graham-campbell/throttle": "^7.0",
        "illuminate/console": "^5.5",
        "illuminate/support": "^5.5",
        "react/zmq": "0.2.*|0.3.*"
    },
    "autoload": {
        "psr-4": {
            "Askedio\\LaravelRatchet\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Askedio\\LaravelRatchet\\Providers\\LaravelRatchetServiceProvider"
            ]
        }
    }
}
